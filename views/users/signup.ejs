<%- include('../partials/header.ejs') %>
<ul>
    <% feedbacks.forEach((feedback) => { %>
      <li style="color:blue;">  <%= feedback %> </li>
    <% }); %>
</ul>
<h1> Sign Up Page</h1>
<div class="form-group">
<form method="POST" action ="/auth/registration">
        <label for="email">Email Address:</label><br>
        <input class="form-control"  type="email" id="email" name="email" value="<%= email %>"><br>
        <label for="password">Password:</label><br>
        <input  class="form-control"  type="password" id="password" name="password"><br><br>
        <label for="password2">Password:</label><br>
        <input class="form-control"  type="password" id="password2" name="verification"><br><br>
        <input type="submit" class="btn btn-primary" value="Submit">
</form>
</div>

<%- include('../partials/footer.ejs') %>

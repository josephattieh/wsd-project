<%- include('../partials/header.ejs') %>
<ul>
    <% feedbacks.forEach((feedback) => { %>
      <li style="color:blue;">  <%= feedback %> </li>
    <% }); %>
</ul>
<h1> Login Page</h1>
<div class="form-group">
<form method="POST" action ="/auth/login" class="">
    <label for="email">Email Address:</label><br>
    <input class="form-control"  type="email" id="email" name="email" value="<%= email %>" ><br>
    <label for="password">Password:</label><br>
    <input class="form-control"  type="password" id="password" name="password"><br><br>
    <input class="btn btn-primary"  type="submit" value="Submit">
</form>
</div>
<div class="form-group">
<form method="GET" action ="/auth/registration" class="">
    <input class="btn btn-primary" type="submit" value="Register">
</form>
</div>
<%- include('../partials/footer.ejs') %>


